Aluno {
  _id: string
  nome: string
  idade: number
  email: string
  telefone: string
  cidade: string
  bairro: string
  estado: ENUM<PR...>
  experiencias: Experiencias[]
  certificados: Certtificados[]
  curso: ENUM<ENSINO_FUNDAMENTAL, ENSINO_MEDIO, CURSO_TECNICO, FACULDADE>
  empresa: Empresa[]
}

Empresa {
  _id: string
  nome: string
  email: string
  telefone: string
  CNPJ: string
  cidade: string
  bairro: string
  estado: ENUM<PR...>
}

Blog {
  _id: string
  titulo: string
  descricao: string
  texto: string
  autor: string
}